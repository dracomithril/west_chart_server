{
  "name": "west_chart_server",
  "version": "2.0.2",
  "description": "server api side of application",
  "main": "src/index.js",
  "engines": {
    "node": "8.x",
    "yarn": "1.6.0"
  },
  "scripts": {
    "start:srv": "node scripts/start_srv.js",
    "start": "node src/index.js",
    "test": "jest --env=jsdom",
    "test-cli": "node scripts/test.js --env=jsdom",
    "lint": "eslint \"**/*.@(js|jsx)\"",
    "format": "npm run lint -- --fix"
  },
  "dependencies": {
    "body-parser": "1.18.3",
    "compression": "^1.7.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "morgan": "^1.9.0",
    "request": "^2.87.0",
    "request-promise-native": "1.0.5",
    "spotify-web-api-node": "3.0.0",
    "winston": "^2.4.1"
  },
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-eslint": "8.2.5",
    "babel-jest": "23.2.0",
    "babel-preset-es2015": "^6.24.1",
    "dotenv": "5.0.1",
    "dotenv-expand": "^4.2.0",
    "eslint": "4.19.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-flowtype": "2.49.3",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-json": "^1.2.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-prettier": "^2.6.1",
    "eslint-plugin-react": "^7.10.0",
    "jest": "^23.2.0",
    "prettier": "^1.11.1",
    "sinon": "^4.4.9"
  },
  "author": "dracomithril",
  "license": "MIT",
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "jest": {
    "collectCoverage": false,
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](build|docs|node_modules|scripts|coverage)[/\\\\]"
    ],
    "notify": true,
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "verbose": true
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/dracomithril/west_chart_server.git"
  },
  "bugs": {
    "url": "https://github.com/dracomithril/west_chart_server/issues"
  },
  "homepage": "https://github.com/dracomithril/west_chart_server#readme"
}

{
  "name": "west_chart_server",
  "version": "1.0.1",
  "description": "server api side of application",
  "main": "src/index.js",
  "engines": {
    "node": "8.x",
    "yarn": "1.6.0"
  },
  "scripts": {
    "start:srv": "node scripts/start_srv.js",
    "start": "node src/index.js",
    "test": "node scripts/test.js --env=jsdom",
    "lint": "eslint \"**/*.@(js|jsx)\"",
    "format": "npm run lint -- --fix"
  },
  "dependencies": {
    "body-parser": "1.18.2",
    "compression": "^1.7.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "ejs": "^2.5.8",
    "express": "^4.16.3",
    "express-winston": "^2.5.0",
    "mongodb": "^3.0.5",
    "request": "^2.85.0",
    "request-promise-native": "1.0.5",
    "serve-favicon": "2.4.5",
    "serve-static": "1.13.2",
    "spotify-web-api-node": "3.0.0",
    "winston": "^2.4.1"
  },
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-eslint": "8.2.2",
    "babel-jest": "22.4.3",
    "babel-preset-es2015": "^6.24.1",
    "dotenv": "5.0.1",
    "dotenv-expand": "^4.2.0",
    "eslint": "4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-flowtype": "2.46.1",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-json": "^1.2.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.7.0",
    "jest": "^22.4.3",
    "prettier": "^1.11.1",
    "sinon": "^4.4.9"
  },
  "author": "dracomithril",
  "license": "MIT",
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "jest": {
    "collectCoverage": false,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "testPathIgnorePatterns": [
      "[/\\\\](build|docs|node_modules|scripts|coverage)[/\\\\]"
    ],
    "notify": true,
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "verbose": true
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/dracomithril/west_chart_server.git"
  },
  "bugs": {
    "url": "https://github.com/dracomithril/west_chart_server/issues"
  },
  "homepage": "https://github.com/dracomithril/west_chart_server#readme"
}
